{
  "name": "black-jack",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "server": "tsup --watch --onSuccess \"node build/server.cjs\"",
    "dev": "NODE_ENV=development concurrently 'vite dev' 'tsup --watch --onSuccess \"node build/server.cjs\"'",
    "build": "concurrently 'vite build' 'tsup'",
    "start": "NODE_ENV=production node build/server.cjs"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "concurrently": "^8.2.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "lucide-react": "^0.277.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
